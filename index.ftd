-- ds.site-page: ReRooted Films
spacing: $ds.spaces.vertical-gap.zero
inset: $ds.spaces.inset-square.zero
theme-switcher: false
margin: $ds.spaces.vertical-gap.zero
document-title: ReRooted Films || Home
document-description: ReRooted Films Website.
document-image: https://film-studio.fifthtry.site/-/film-studio.fifthtry.site/assets/card-2-dark.jpg




-- ds.column:
align-content: top-center
spacing: $ds.spaces.vertical-gap.zero
margin: $ds.spaces.vertical-gap.zero
inset: $ds.spaces.inset-square.zero

	-- hero.hero-with-background: ReRooted.Films
	bg-image: $assets.files.assets.banners.homepage.png
	sub-title: Rooted in Stories.Grounded in Sustainability.
	min-height.fixed.vh: 100
	color-hero: #ffffff
	color-text: #ffffff
	
	
	
	
	-- ds.column:
	radius: $ds.radius.zero
	inset: $ds.spaces.inset-square.extra-large
	spacing: $ds.spaces.vertical-gap.extra-large
	
		-- ds.heading-large: What We Offer
		
		-- ds.row:
		wrap: true
		spacing: $ds.spaces.horizontal-gap.extra-extra-large
		spacing if { ftd.device == "mobile" }: $ds.spaces.horizontal-gap.zero
		inset: $ds.spaces.inset-tall.zero-medium
		
			-- card.icon-card: Audio
			icon: $assets.files.assets.icons.podcast_icon.svg
			width: hug-content
			card-icon: true
			
			PODCASTS. PSA.JINGLES.
			
			-- card.icon-card: Audio Visual
			icon: $assets.files.assets.icons.film.svg
			width: hug-content
			card-icon: true
			
			DOCUMENTARIES. FICTION FILMS. ADS. MUSIC VIDEOS.
			
			-- card.icon-card: Text
			icon: $assets.files.assets.icons.book_icon.svg
			width: hug-content
			card-icon: true
			
			REPORTS. SCRIPTS. BOOKS.
			
		-- end: ds.row

	-- end: ds.column




	-- ds.column:
	radius: $ds.radius.zero
	inset: $ds.spaces.inset-tall.zero-medium
	spacing: $ds.spaces.vertical-gap.extra-large
	margin: $ds.spaces.vertical-gap.extra-large
	

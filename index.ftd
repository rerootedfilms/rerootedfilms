			width.fixed.px: 300

			Tons of material diverted from landfill

			-- card.icon-card:
			icon: $assets.files.assets.icons.bottle.svg
			card-icon: true
			width.fixed.px: 300

			Plastic water bottles avoided

			-- end: ftd.row

			/-- hero.hero-sticky-image: This website takes up ___ C02
			sticky-image: https://film-studio.fifthtry.site/-/film-studio.fifthtry.site/assets/icon-2-dark.svg
			sub-title-1: We follow a low waste process on all our shoots.
			sub-title-2: We even audit our waste in the form of a certificate.
			
		-- end: ds.column

		-- ds.column:
		inset if { ftd.device != "mobile" }: $ds.spaces.inset-tall.large
		spacing: $ds.spaces.vertical-gap.large
		background.solid: $inherited.colors.background.step-1
		radius: $ds.radius.zero
		
			-- ds.heading-large: Meet Our Team
			align: center
			
			-- team.member:
			team-member: $team-member
			
		-- end: ds.column

